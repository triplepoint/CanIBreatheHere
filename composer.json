{
    "name":        "triplepoint/CanIBreatheHere",

    "description": "Single-purpose site that answers the question \"Can I breathe here?\".",

    "keywords": [
         "single-purpose", "breathe", "air", "oxygen"
     ],

    "homepage":    "https://github.com/triplepoint/CanIBreatheHere",

    "license": "MIT",

    "authors": [
        {
            "name":     "Jonathan Hanson",
            "email":    "jonathan@jonathan-hanson.org",
            "homepage": "http://www.jonathan-hanson.org/",
            "role":     "Developer"
        }
    ],

    "support": {
        "email":  "jonathan@jonathan-hanson.org",
    	"issues": "https://github.com/triplepoint/CanIBreatheHere/issues",
    	"source": "https://github.com/triplepoint/CanIBreatheHere"
    },

    "require": {
        "php":                                       ">=5.6",
        "silex/silex":                               "1.0.*@dev",
        "triplepoint/php-units-of-measure":          "1.1",
        "mustache/mustache":                         "2.1.*",
        "monolog/monolog":                           "1.17.2"
    },

    "require-dev": {
        "phpunit/phpunit":                           "5.1.*",
        "squizlabs/php_codesniffer":                 "2.5.*"
    },

    "autoload": {
        "psr-4": {
            "CanIBreatheHere\\": "source/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "CanIBreatheHereTest\\": "tests/"
        }
    }
}
